<div id="Dexcom_Form">
  <% if @readings.nil? %>
    <%= form_tag import_readings_path, multipart: true do %>
      <%= file_field_tag :file %>
      <%= submit_tag "Import Dexcom Data" %>
    <% end %>
  <% end %>
</div>


<section class="main">
  <div class="sample">
    <%= @readings.each do |r| %>
      <%= r.glucose_value %>
    <% end %>
  </div>
  <%= link_to "Dexcom Readings", reading_path(@readings) %>
  <div id="chart_div">
  </div>
</section>
