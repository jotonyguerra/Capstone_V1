<div class="main mt-4">
  <div class="container">
    <div class="row">
      <section class="col align-items-center">
        <div class="form-group">
          <div class="input-group">
              <%= form_tag import_readings_path, multipart: true do %>
              <%= file_field_tag :file %>
              <%= button_tag(type: 'submit', class: "btn btn-default") do %>
                <i class="icon-ok icon-white"></i> Import Data
                <% end %>
              <% end %>
          </div> <!--input-group-->
        </div> <!--form-group-->
      </section> <!-- col -->
    </div> <!-- row -->
</div> <!-- container -->

<div class="container">
  <!-- chart div is for google charts display -->
  <div id="chart_wrap"><div id="chart"></div></div>
</div>
</div>
